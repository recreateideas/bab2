{
  "name": "baboon",
  "productName": "Baboon",
  "version": "0.1.0",
  "author": "recreateideas",
  "description": "an awesome MongoDB UI",
  "private": true,
  "main": "./main.js",
  "keywords": [
    "baboon"
  ],
  "dependencies": {
    "electron-context-menu": "0.10.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "watch-css": "node-sass-chokidar src/scss -o src/css --watch --recursive",
    "start-js": "concurrently \"mongod\" \"babel src --out-dir babel_compiled --watch\" \"electron .\"",
    "start-js-server": "concurrently \"mongod\" \"babel src --out-dir babel_compiled --watch\"",
    "start-js-browser": "concurrently \"mongod\" \"babel src --out-dir babel_compiled --watch\" \"react-scripts start\"",
    "bundle-ui-prod": "webpack --env.mode=production",
    "bundle-ui-dev": "webpack --env.mode=development",
    "compile-service": "babel src/server --out-dir public/server",
    "build-css": "node-sass-chokidar src/scss/App.scss src/css/App.css",
    "build-prod": "npm-run-all build-css compile-service bundle-ui-prod",
    "build-dev": "npm-run-all build-css compile-service bundle-ui-dev",
    "prod": "electron .",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "package-mac": "electron-packager . baboon --overwrite --platform=darwin --arch=x64 --icon=/Users/claudio/baboon/assets/baboon.iconset --prune=true --out=release-builds --ignore=users --ignore=static --ignore=babel_compiled --ignore=installers --ignore=backups --ignore=src --ignore=assets --ignore=webpack.config.js --ignore=.eslintrc.json --ignore=.babelrc --ignore=.gitignore --ignore=.vscode --ignore=.env --ignore=Icon? --ignore=to-do",
    "package-linux": "electron-packager . baboon --overwrite --asar --platform=linux --arch=x64 --icon=/Users/claudio/baboon/assets/baboon.iconset --prune=true --out=release-builds --ignore=users --ignore=static --ignore=babel_compiled --ignore=installers --ignore=backups --ignore=src --ignore=assets --ignore=webpack.config.js --ignore=.eslintrc.json --ignore=.babelrc --ignore=.gitignore --ignore=.vscode --ignore=.env --ignore=Icon? --ignore=to-do",
    "package-win": "electron-packager . baboon --overwrite --asar --platform=win32 --arch=ia32 --icon=/Users/claudio/baboon/assets/baboon.iconset --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Baboon\"",
    "installer-mac": "electron-installer-dmg /Users/claudio/baboon/release-builds/Baboon-darwin-x64/Baboon.app --out=release-builds/installers Baboon",
    "installer-win": "",
    "installer-linux": "electron-packager . baboon --platform linux --arch x64 --out release-builds/installers --ignore=webpack.config.js",
    "package-mac-old": "electron-packager . --overwrite --platform=darwin --asar --arch=x64 --icon=/Users/claudio/baboon/assets/baboon.iconset --prune=true --out=release-builds --ignore=installers --ignore=backups --ignore=dist --ignore=src --ignore=assets --ignore=webpack.config.js --ignore=.eslintrc.json --ignore=.babelrc --ignore=.gitignore --ignore=.vscode",
    "mac-prod": "run-s build-prod package-mac installer-mac"
  },
  "proxy": "http://localhost:5001",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "ajv": "5.0.0",
    "axios": "0.18.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-css-modules-transform": "1.6.1",
    "babel-plugin-import-static-files": "1.0.4",
    "babel-plugin-inline-import-data-uri": "1.0.1",
    "babel-plugin-inline-json": "1.2.2",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "body-parser": "1.18.3",
    "celebrate": "8.0.0",
    "concurrently": "3.5.0",
    "connect": "3.6.6",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "electron": "2.0.0",
    "electron-css-reload": "1.0.8",
    "electron-devtools-installer": "2.2.4",
    "electron-packager": "12.1.0",
    "electron-reload": "1.2.5",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "http-proxy": "1.17.0",
    "image-webpack-loader": "4.4.0",
    "jsrsasign": "8.0.12",
    "jsrsasign-util": "1.0.0",
    "mini-css-extract-plugin": "0.4.4",
    "moment": "2.22.2",
    "moment-timezone": "0.5.21",
    "mongodb": "3.0.10",
    "mongodb-extended-json": "1.10.0",
    "mongodb-query-parser": "1.1.1",
    "multer": "1.3.1",
    "node-sass": "4.9.4",
    "node-sass-chokidar": "1.3.3",
    "nodemon": "1.18.2",
    "npm-run-all": "4.1.3",
    "react": "16.3.2",
    "react-bootstrap": "0.32.1",
    "react-datepicker": "1.5.0",
    "react-dom": "16.3.2",
    "react-json-pretty": "1.7.6",
    "react-redux": "5.0.7",
    "react-scripts": "1.1.4",
    "react-tooltip": "3.6.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "request": "2.79.0",
    "sass-loader": "7.1.0",
    "socket.io-client": "2.1.1",
    "socket.io-file-client": "2.0.2",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.23.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.0.0"
  }
}
